<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD GWT 2.7.0//EN"
    "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='basic'>
  <!-- Inherit the core Web Toolkit stuff. -->
  <inherits name='com.google.gwt.user.User' />

  <!-- Other module inherits -->
  <inherits name='com.sencha.gxt.ui.GXT' />

  <!-- GXT Normalizing Stylesheet -->
  <stylesheet src="reset.css" />

  <!-- GXT Theme -->
  <!-- <inherits name='com.sencha.gxt.theme.blue.Blue'/> -->
  <!-- <inherits name='com.sencha.gxt.theme.gray.Gray' /> -->
  <inherits name="com.sencha.gxt.theme.neptune.Theme" />
  <inherits name="com.sencha.gxt.theme.triton.Theme" />

  <!-- Specify the app entry point class. -->
  <entry-point class='com.clubdesk.gxt.client.basicEntryPoint' />

  <!-- Specify the paths for translatable code -->
  <source path='client' />
  <source path='shared' />
  
  <!-- Logging options -->
  <!-- <set-property name="gwt.logging.logLevel" value="INFO"/> -->
  <!-- <set-property name="gwt.logging.enabled" value="TRUE"/> -->  
  <!-- <set-property name="gxt.logging.enabled" value="true" />  -->

  <set-property name="gxt.device" value="desktop"/>
  <set-property name="gxt.user.agent" value="chrome"/>
  <set-property name="user.agent" value="gecko1_8"/>

</module>
